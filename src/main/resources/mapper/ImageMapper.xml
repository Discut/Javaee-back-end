<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ybuse.schoolbackend.image.mapper.ImageMapper">

    <resultMap id="BaseResultMap" type="com.ybuse.schoolbackend.image.domain.po.ImagePo">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="imName" column="im_name" jdbcType="VARCHAR"/>
            <result property="imPath" column="im_path" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
